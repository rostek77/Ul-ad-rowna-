{
puts("Podaj wartość a1");
scan ("%lf",&a1);

puts("Podaj wartość a2");
scan ("%lf",&a2);

puts("Podaj wartość b1");
scan ("%lf",b1);

puts("Podaj wartość b2");
scan ("%d",&b2);

puts("Podaj wartość c1");
scan ("%d",&c1);

puts("Podaj wartość c2");
scan ("%lf",&c2);


int main()
{
	duble a1,a2,b1,b2,c1,c2;

wczytaj a1,a1,a2,b1,b2,c1,c2;

